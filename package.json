{
  "name": "@octetstream/invariant",
  "description": "Declarative errors throwing for Node.js",
  "version": "1.0.0",
  "main": "invariant.js",
  "author": "Nick K.",
  "license": "MIT",
  "keywords": [
    "invariant",
    "test",
    "errors"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/invariant.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/invariant/issues"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint test.js invariant.js",
    "test": "node_modules/.bin/ava test.js",
    "codecov": "node_modules/.bin/codecov",
    "coverage": "node_modules/.bin/nyc yarn test",
    "report": "yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:ci": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "ci": "yarn coverage && yarn report:ci && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "engines": {
    "node": ">=7.6"
  },
  "dependencies": {
    "lodash.isfunction": "3.0.8",
    "lodash.isstring": "4.0.1",
    "sprintf-js": "1.1.1"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "1.0.1",
    "ava": "0.21.0",
    "codecov": "2.2.0",
    "eslint": "3.19.x",
    "husky": "0.14.3",
    "nyc": "11.0.3"
  }
}
